<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- slide bar 有效内容背景 -->
    <attr name="backgroundColor" format="color|reference" />
    <!-- slide bar　描边 -->
    <attr name="strokeColor" format="color|reference" />
    <!-- slide bar 文字颜色 -->
    <attr name="textColor" format="color|reference" />
    <!-- slide bar　文字大小 -->
    <attr name="textSize" format="dimension" />
    <!-- slide bar 选中文字颜色 -->
    <attr name="selectTextColor" format="color|reference" />
    <!-- slide bar 选中文字大小 -->
    <attr name="selectTextSize" format="dimension" />
    <!-- slide bar　选中时候提示文字颜色 -->
    <attr name="hintTextColor" format="color|reference" />
    <!-- slide bar　选中时候提示文字大小 -->
    <attr name="hintTextSize" format="dimension" />
    <!-- slide bar　选中时候提示圆形半径 -->
    <attr name="hintCircleRadius" format="dimension" />
    <!-- slide bar　选中时候提示圆形颜色 -->
    <attr name="hintCircleColor" format="color|reference" />
    <!-- slide bar　选中时候提示背景形状 -->
    <attr name="hintShape" format="enum">
        <enum name="circle" value="0" />
        <enum name="rectangle" value="1" />
    </attr>
    <!-- slide bar　选中时候波浪颜色 -->
    <attr name="waveColor" format="color|reference" />
    <!-- slide bar　选中时候波浪半径 -->
    <attr name="waveRadius" format="dimension" />
    <!-- slide bar　里面文字上＋下padding -->
    <attr name="contentPadding" format="dimension" />
    <!-- slide bar　上+下+右padding -->
    <attr name="barPadding" format="dimension" />
    <!-- slide bar　宽度 -->
    <attr name="barWidth" format="dimension" />
    <!--wheelview 属性-->
    <attr name="itemHeight" format="dimension" />
    <attr name="unselectTextSize" format="dimension" />
    <attr name="unselectTextColor" format="color" />
    <attr name="wheelOffset" format="integer" />
    <attr name="dividerWidth" format="dimension" />
    <attr name="dividerHeight" format="dimension" />
    <attr name="dividerColor" format="color" />
    <attr name="dividerMargin" format="dimension"/>

    <declare-styleable name="AZWaveSideBarView">
        <!-- slide bar 有效内容背景 -->
        <attr name="backgroundColor" />
        <!-- slide bar　描边 -->
        <attr name="strokeColor" />
        <!-- slide bar 文字颜色 -->
        <attr name="textColor" />
        <!-- slide bar　文字大小 -->
        <attr name="textSize" />
        <!-- slide bar 选中文字颜色 -->
        <attr name="selectTextColor" />
        <!-- slide bar 选中文字大小 -->
        <attr name="selectTextSize" />
        <!-- slide bar　选中时候提示文字颜色 -->
        <attr name="hintTextColor" />
        <!-- slide bar　选中时候提示文字大小 -->
        <attr name="hintTextSize" />
        <!-- slide bar　选中时候提示圆形半径 -->
        <attr name="hintCircleRadius" />
        <!-- slide bar　选中时候提示圆形颜色 -->
        <attr name="hintCircleColor" />
        <!-- slide bar　选中时候波浪颜色 -->
        <attr name="waveColor" />
        <!-- slide bar　选中时候波浪半径 -->
        <attr name="waveRadius" />
        <!-- slide bar　里面文字上＋下padding -->
        <attr name="contentPadding" />
        <!-- slide bar　上+下+右padding -->
        <attr name="barPadding" />
        <!-- slide bar　宽度 -->
        <attr name="barWidth" />
    </declare-styleable>

    <declare-styleable name="AZSideBarView">
        <!-- slide bar 有效内容背景 -->
        <attr name="backgroundColor" />
        <!-- slide bar　描边 -->
        <attr name="strokeColor" />
        <!-- slide bar 文字颜色 -->
        <attr name="textColor" />
        <!-- slide bar　文字大小 -->
        <attr name="textSize" />
        <!-- slide bar 选中文字颜色 -->
        <attr name="selectTextColor" />
        <!-- slide bar 选中文字大小 -->
        <attr name="selectTextSize" />
        <!-- slide bar　选中时候提示文字颜色 -->
        <attr name="hintTextColor" />
        <!-- slide bar　选中时候提示文字大小 -->
        <attr name="hintTextSize" />
        <!-- slide bar　选中时候提示圆形半径 -->
        <attr name="hintCircleRadius" />
        <attr name="hintShape" />
        <!-- slide bar　选中时候提示圆形颜色 -->
        <attr name="hintCircleColor" />
        <!-- slide bar　里面文字上＋下padding -->
        <attr name="contentPadding" />
        <!-- slide bar　上+下+右padding -->
        <attr name="barPadding" />
        <!-- slide bar　宽度 -->
        <attr name="barWidth" />
    </declare-styleable>


    <!--myDatePicker-->
    <declare-styleable name="MyDatePicker">
        <attr name="minYear" format="integer" />
        <attr name="maxYear" format="integer" />
    </declare-styleable>
    <!--自定义选择器-->
    <declare-styleable name="MyWheelView">
        <attr name="selectTextSize" />
        <attr name="selectTextColor" />
        <attr name="itemHeight" />
        <attr name="unselectTextSize" />
        <attr name="unselectTextColor" />
        <attr name="wheelOffset" />
        <attr name="dividerWidth" />
        <attr name="dividerHeight" />
        <attr name="dividerColor" />
    </declare-styleable>

    <declare-styleable name="WheelRecyclerView">
        <attr name="selectTextSize" />
        <attr name="selectTextColor" />
        <attr name="itemHeight" />
        <attr name="unselectTextSize" />
        <attr name="unselectTextColor" />
        <attr name="wheelOffset" />
        <attr name="dividerWidth" />
        <attr name="dividerHeight" />
        <attr name="dividerColor" />
        <attr name="dividerMargin"/>
    </declare-styleable>
    <!--折叠textview-->
    <declare-styleable name="FoldTextView">
        <attr name="showMaxLine" format="integer" />
        <attr name="tipGravity" format="integer" />
        <attr name="tipColor" format="reference|color" />
        <attr name="tipClickable" format="boolean" />
        <attr name="foldText" format="string" />
        <attr name="expandText" format="string" />
        <attr name="showTipAfterExpand" format="boolean" />
        <attr name="isSetParentClick" format="boolean" />
    </declare-styleable>


    <declare-styleable name="EnhanceTabLayout">
        <attr name="tab_Mode" format="enum">
            <enum name="mode_fixed" value="1" />
            <enum name="mode_scrollable" value="2" />
        </attr>
        <attr name="tabIndicatorColor" format="color" />
        <attr name="tabSelectTextColor" format="color" />
        <attr name="tabTextColor" format="color" />
        <attr name="tabIndicatorHeight" format="dimension" />
        <attr name="tabIndicatorWidth" format="dimension" />
        <attr name="tabTextSize" format="dimension" />
        <attr name="tabMContentStart" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MyBanner">
        <attr name="delay_time" format="integer" />
        <attr name="scroll_time" format="integer" />
        <attr name="is_auto_play" format="boolean" />
        <attr name="title_background" format="color|reference" />
        <attr name="title_textcolor" format="color" />
        <attr name="title_textsize" format="dimension" />
        <attr name="title_height" format="dimension" />
        <attr name="indicator_width" format="dimension" />
        <attr name="indicator_height" format="dimension" />
        <attr name="indicator_margin" format="dimension" />
        <attr name="indicator_marginbottom" format="dimension" />
        <attr name="indicator_drawable_selected" format="reference" />
        <attr name="indicator_drawable_unselected" format="reference" />
        <attr name="banner_layout" format="reference" />
        <attr name="banner_default_image" format="reference" />
        <attr name="image_scaletype" format="enum">
            <enum name="center" value="0" />
            <enum name="center_crop" value="1" />
            <enum name="center_inside" value="2" />
            <enum name="fit_center" value="3" />
            <enum name="fit_end" value="4" />
            <enum name="fit_start" value="5" />
            <enum name="fit_xy" value="6" />
            <enum name="matrix" value="7" />
        </attr>
    </declare-styleable>
    <item name="bannerContainer" type="id" />
    <item name="bannerDefaultImage" type="id" />
    <item name="bannerTitle" type="id" />
    <item name="bannerViewPager" type="id" />
    <item name="circleIndicator" type="id" />
    <item name="indicatorInside" type="id" />
    <item name="numIndicator" type="id" />
    <item name="numIndicatorInside" type="id" />
    <item name="titleView" type="id" />
</resources>